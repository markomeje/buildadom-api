{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0.1",
        "africastalking/africastalking": "^3.0",
        "aws/aws-sdk-php": "^3.277",
        "guzzlehttp/guzzle": "^7.5",
        "halaxa/json-machine": "^1.1",
        "laracraft-tech/laravel-useful-additions": "*",
        "laravel-notification-channels/africastalking": "^3.1.0",
        "laravel/framework": "^9.19",
        "laravel/horizon": "^5.22",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.12",
        "mane-olawale/termii": "^1.2",
        "php-open-source-saver/jwt-auth": "^2.0",
        "propaganistas/laravel-phone": "^5.1",
        "queueworker/sansdaemon": "^1.2",
        "smknstd/fakerphp-picsum-images": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "pestphp/pest": "^1.23",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "tightenco/duster": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "chmod -R ug+rwx storage bootstrap/cache"
        ],
        "pint": [
            "./vendor/bin/pint --config pint.json"
        ],
        "stan": [
            "./vendor/bin/phpstan --level=9 analyse -- ./app"
        ],
        "test": [
            "./vendor/bin/pest"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "platform-check": false,
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
